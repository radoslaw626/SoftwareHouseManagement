@{
    Layout = null;
}
<html lang="en">
<link type="text/css" id="dark-mode" rel="stylesheet" href="">
<head>
    <style id="stndz-style"></style>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Signin Template for Bootstrap</title>

    <link rel="canonical" href="https://getbootstrap.com/docs/4.0/examples/sign-in/">
    <style>
        

        </style>
    <!-- Bootstrap core CSS -->
    <link href="~/assets/css/bootstrap-min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="~/assets/css/sign-in.css" rel="stylesheet">
</head>

<body class="text-center">
<form class="form-signin">
    <h1 class="h3 mb-3 font-weight-normal">Zarejestruj się</h1><br/>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" id="inputEmail" class="form-control" placeholder="Email" required="" autofocus="">
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" id="inputPassword" class="form-control" placeholder="Hasło" required="" style="margin-bottom: -1px !important;">
    <input type="password" id="inputPassword" class="form-control" placeholder="Powtórz hasło" required="">
    <br />
    <button class="btn btn-lg btn-primary btn-block" type="submit">Zarejestruj się</button>
</form>


</body>
<app-content ng-version="11.1.0"></app-content>
</html>